#, qt-format
msgctxt "RomanNumerals|"
msgid "Roman value: %1"
msgstr "罗马数字：%1"

#: activities/roman_numerals/RomanNumerals.qml:376
#, qt-format
msgctxt "RomanNumerals|"
msgid "Arabic value: %1"
msgstr "阿拉伯数字：%1"

